@import '../../styles/colors'
.mainContent
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: column;
  width: 100%;
  height: 100;
  max-height: 100%;
  margin: 0 calc(35/1300*100%);
  padding-top: 20px;
  &__title
    align-self: center;
  &__list
    display: flex;
    flex-wrap: wrap;
    width: 100%;
    max-height: 80%;
    list-style: none;
    overflow: hidden;
  &__listItem
    display: flex;
    flex-wrap: wrap;
    width: 30%;
    height: 80px;
    margin-right: 3%;
    border-bottom: 1px solid elements-background;
  &__icon
    width: 30px;
    height: 30px;
    margin-right: 10px;
  &__pagination
    display: flex;
    align-self: center;
    list-style: none;
